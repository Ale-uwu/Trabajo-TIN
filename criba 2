#include<stdio.h>
#include<stdlib.h>
#include<math.h>
     void main()
{
    
    int n;
        printf("Introduce un n√∫mero entero:\n");
        scanf("%d",&n);

        int v[n];
        int i;
        for(i=2;i<n;i++)
        {
            v[i]=i;
            printf("%d\n",v[i]);
         } 
         int j;

        for(i=2;i<n;i++)
    {
        for(j=2;i*j<n;j++)
        {
            v[i*j]=0;
        }
    }
        for(i=2;i<n;i++)
        {
          if (v[i]!=v[i*j])  
          printf("\nNumeros primos: %*d\n",v[i]);
        }
     }
