#include <stdio.h>
#include <stdbool.h>

void main()
{
    int n,i,j;
    printf("Introduce el valor del vector:\n");
    scanf("%i",&n);
    bool vector [n];
    for (i=0;i<n;i++)
    {
        vector [i]=true;
    }
    for (i=2;i<n;i++)
    {
        for (j=2;i*j<n;j++)
        {
            vector [i*j]=false;
        }
    }
    for (i=2;i<n;i++)
    {
        if (vector[i])
            printf("%d",vector[i]);
        
    }
}
